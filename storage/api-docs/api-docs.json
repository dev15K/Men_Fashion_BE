        "/api/admin/products/create": {
            "post": {
                "tags": [
                    "Admin Product"
                ],
                "summary": "Create product",
                "description": "Create a new product",
                "operationId": "133be1e44de223e0af7faceb8ac3996a",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "description": "Name of the product",
                                        "type": "string",
                                        "example": "Sample Product"
                                    },
                                    "short_description": {
                                        "description": "Short description of the product",
                                        "type": "string",
                                        "example": "This is a short description."
                                    },
                                    "description": {
                                        "description": "Detailed description of the product",
                                        "type": "string",
                                        "example": "This is a detailed description of the product."
                                    },
                                    "parent_id": {
                                        "description": "Category ID for the product",
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "thumbnail": {
                                        "description": "Thumbnail image of the product",
                                        "type": "string",
                                        "format": "binary"
                                    },
                                    "gallery": {
                                        "description": "Gallery images of the product",
                                        "type": "array",
                                        "items": {
                                            "type": "string",
                                            "format": "binary"
                                        }
                                    },
                                    "price": {
                                        "description": "Price of the product",
                                        "type": "number",
                                        "format": "float",
                                        "example": 19.99
                                    },
                                    "sale_price": {
                                        "description": "Sale price of the product",
                                        "type": "number",
                                        "format": "float",
                                        "example": 14.99
                                    },
                                    "quantity": {
                                        "description": "Quantity of the product",
                                        "type": "integer",
                                        "example": 100
                                    },
                                    "status": {
                                        "description": "Status of the product (e.g., available, out of stock)",
                                        "type": "integer",
                                        "enum": [
                                            1,
                                            2,
                                            3
                                        ],
                                        "example": 1
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Product created successfully"
                    },
                    "400": {
                        "description": "Invalid input data"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
